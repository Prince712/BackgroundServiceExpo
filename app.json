{
  "expo": {
    "name": "background-services",
    "slug": "background-services",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "audio",
          "processing"
        ]
      }
    },
    "notification": {
      "iosDisplayInForeground": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-background-fetch"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81b0f40d-9a07-4da3-93e2-a0c12e139f60"
      }
    },
    "android": {
      "package": "com.pravin.dabekar7.backgroundservices",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    }
  }
}
